
(define implementations
  '(((name . chibi) (command . "chibi-scheme -I srfi"))
    ((name . chicken)
     (command . "csc -include-path ./srfi -X r7rs -R r7rs")
     (library-command . "csc -include-path ./srfi -X r7rs -R r7rs -s -J"))
    ((name . cyclone)
     (command . "cyclone -I .")
     (library-command . "cyclone -I ."))
    ((name . gambit)
     (command . "gsc -:r7rs,search=. -exe")
     (library-command .  "gsc -:r7rs -dynamic"))
    ((name . gerbil) (command . "gxi"))
    ((name . gauche) (command . "gosh"))
    ((name . guile) (command . "guile --fresh-auto-compile --r7rs -L ."))
    ((name . kawa) (command . "kawa"))
    ((name . loko) (command . "loko -feval -std=r7rs --compile"))
    ((name . mit-scheme) (command . "mit-scheme --load"))
    ((name . racket) (command . "racket -I r7rs --make -S . --script"))
    ((name . sagittarius) (command . "sash"))
    ((name . stklos) (command . "stklos -I ."))
    ((name . skint) (command . "skint --program"))
    ((name . tr7) (command . "tr7i"))))
