                    sh 'for f in *.log; do cp -- "$f" "reports/{{name}}-$f.log"; done'
                    sh 'ls reports'
                    stash name: 'reports', includes: 'reports/*'
                    archiveArtifacts artifacts: 'reports/*.log'
                    // Clean up possible executables
                    sh 'rm -rf srfi-test/{{number}}'

                }
            }
        }
